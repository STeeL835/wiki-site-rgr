@model WikiSite.PL.ASP.Models.ArticleVM

@{
    ViewBag.Title = "Создание статьи";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1 class="page-header">
    @ViewBag.Title
</h1>

<form method="post" class="form-override">
    @Html.Partial("~/Views/Shared/ArticlesEditorMDPartial.cshtml", Model)
    <div class="form-group">
        <div class="col-xs-12">
            <input type="submit" value="Отправить" class="btn btn-default button-override" />
        </div>
    </div>
</form>